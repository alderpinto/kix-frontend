{
  "name": "@kix/frontend",
  "version": "0.0.15",
  "description": "KIXng Frontend Webapplication.",
  "config": {
    "bundlefile": "kix-frontend.tar.gz"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build-cucumber": "rm -R dist/ && tsc && cp -R features/ dist/",
    "test-cucumber": "rm -R dist/ && tsc && cp -R features/ dist/ && ./node_modules/cucumber/bin/cucumber-js dist/features/",
    "test": "export http_proxy= && export https_proxy= && export NODE_ENV=test && mocha --reporter spec --compilers ts:ts-node/register --require source-map-support/register 'tests/**/*.test.ts'",
    "pub": "gulp && npm version patch && npm publish",
    "bundle": "rm -f ${npm_package_config_bundlefile} && tar cvfz ${npm_package_config_bundlefile} * --exclude-from=.bundleignore",
    "extract-translatables": "./extract-translatables.sh"
  },
  "repository": {
    "type": "git",
    "url": "http://git.intra.cape-it.de/Softwareentwicklung/KIXng/frontend/app.git"
  },
  "author": "c.a.p.e. IT GmbH",
  "dependencies": {
    "@lasso/marko-taglib": "1.0.13",
    "body-parser": "1.18.3",
    "bowser": "2.1.2",
    "compression": "1.7.3",
    "cookie-parser": "1.4.3",
    "csso": "3.5.1",
    "express": "4.16.4",
    "express-force-ssl": "0.3.2",
    "js-plugins": "1.0.0",
    "jsonfile": "5.0.0",
    "jsonwebtoken": "8.5.0",
    "lasso": "3.2.9",
    "lasso-less": "3.0.1",
    "lasso-marko": "2.4.6",
    "lasso-minify": "1.0.4",
    "lasso-minify-css": "1.1.4",
    "marko": "4.15.6",
    "marko-loader": "1.3.3",
    "md5": "2.2.1",
    "memcached": "2.2.2",
    "raml2html": "7.2.2",
    "reflect-metadata": "0.1.13",
    "request": "2.88.0",
    "request-promise": "4.2.2",
    "socket.io": "2.2.0",
    "winston": "3.1.0",
    "winston-daily-rotate-file": "3.3.3"
  },
