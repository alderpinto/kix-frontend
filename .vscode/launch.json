{
    "version": "0.2.0",
    "configurations": [
        {
            "args": [],
            "cwd": "${workspaceRoot}",
            "env": {
                "NODE_ENV": "development"
            },
            "name": "KIXng",
            "program": "${workspaceRoot}/src/server.ts",
            "request": "launch",
            "sourceMaps": true,
            "stopOnEntry": false,
            "type": "node",
