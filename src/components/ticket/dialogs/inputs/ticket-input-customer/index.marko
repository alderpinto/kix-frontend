<if(state.loading)>
    <loading-spinner/>
</if>
<else>
    <form-list
        readonly=(!state.hasContact || state.field.readonly)
        selectedNodes=[state.currentNode]
        nodes=state.nodes
        placeholder=state.placeholder
        on-focusLost("focusLost")
        on-nodesChanged("customerChanged")
        invalid=state.invalid/>
</else>
