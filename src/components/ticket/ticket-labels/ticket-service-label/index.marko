<if(state.serviceId)>
    <key-value-pair showKey=input.hasLabel>
        <@key>${state.label}</@key>
        <@value>
            <if(input.hasText)>
                <span.text>${state.displayValue}</span>
            </if>
            <if(input.hasIcon)>
                <icon object="IncidentState" objectId=state.incidentStateId/>
            </if>
        </@value>
    </key-value-pair>
</if>
