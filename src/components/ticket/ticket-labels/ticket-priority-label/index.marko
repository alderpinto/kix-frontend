<key-value-pair showKey=input.hasLabel>
    <@key>${state.label}</@key>
    <@value>
        <if(input.hasText)>
            <span.text>${state.displayValue}</span>
        </if>
        <if(input.hasIcon)>
            <icon object="Priority" objectId=state.priorityId/>
        </if>
    </@value>
</key-value-pair>
