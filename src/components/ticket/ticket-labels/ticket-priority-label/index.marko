<key-value-pair showKey=input.hasLabel>
    <@key>${state.label}</@key>
    <@value>
        <if(input.hasText)>
            <span.text title=state.displayValue>${state.displayValue}</span>
        </if>
        <if(input.hasIcon)>
            <icon object="Priority" objectId=state.priorityId title=state.displayValue/>
        </if>
    </@value>
</key-value-pair>
