<key-value-pair showKey=input.showLabel>
    <@key>${state.label}</@key>
    <@value>
        <if(input.showLabel)>
            <span.text>${state.displayValue}</span>
        </if>
        <span class="priority-label-${state.priorityId}-style-a">${state.priorityId}</span>
    </@value>
</key-value-pair>
