<key-value-pair showKey=input.showLabel>
    <@key>${state.label}</@key>
    <@value>
        <span.text>${state.displayValue}</span>
        <if(input.showLabel && state.pending)>
            <span.text>
                <ticket-property-label property="PendingTime" value=state.pendingTime/>
            </span>
        </if>
    </@value>
</key-value-pair>
