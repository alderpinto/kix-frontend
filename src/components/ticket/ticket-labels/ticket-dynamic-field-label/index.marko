<key-value-pair showKey=input.showLabel>
    <@key>${state.label}</@key>
    <@info>
        <overlay-info-icon>
            <@title>${state.label}</@title>
            <@content>
                <span>Beschreibung des Feldes ...</span>
            </@content>
        </overlay-info-icon>
    </@info>
    <@value>
        <span.text>${state.displayValue}</span>
    </@value>
</key-value-pair>
