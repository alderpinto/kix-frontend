<widget instanceId=state.instanceId title=state.title actions=state.actions>
    <@icon>
        <span.kix-icon-minus/>
    </@icon>
    <@headerContent>
        <filter predefinedFilter=state.predefinedTableFilter predefinedFilterPlaceholder="Alle Tickets" on-filter('filter')/>
    </@headerContent>
    <@content>
        <if(state.table)>
            <standard-table standardTable=state.table/>
        </if>
    </@content>
</widget>
