<widget instanceId=state.instanceId title=state.title actions=state.actions icon="kix-icon-ticket">
    <@headerFilter>
        <filter predefinedFilter=state.predefinedTableFilter predefinedFilterPlaceholder="Alle Tickets" on-filter('filter')/>
    </@headerFilter>
    <@content>
        <if(state.table)>
            <standard-table standardTable=state.table key="ticket-list-table"/>
        </if>
    </@content>
</widget>
