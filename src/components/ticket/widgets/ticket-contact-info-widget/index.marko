<widget instanceId=input.instanceId>
    <@icon/>
    <@title>
        <if(state.widgetConfiguration)>${state.widgetConfiguration.title}</if>
    </@title>
    <@content>
        <if(state.contact)>
            <contact-info contact=state.contact/>
        </if>
    </@content>
    <@filter/>
    
    <@contentActions/>
</widget>
