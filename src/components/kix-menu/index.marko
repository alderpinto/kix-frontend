<div#menu>
    <ul#main_menu>
        <for(menuEntry in state.menuEntries)>
            <li>
                <a href=menuEntry.link>${menuEntry.text}</a>
            </li>
        </for>
    </ul>
    <ul#main_menu_extra>
        <for(menuEntry in state.menuEntriesExtra)>
            <li>
                <a href=menuEntry.link>${menuEntry.text}</a>
            </li>
        </for>
    </ul>
</div>
