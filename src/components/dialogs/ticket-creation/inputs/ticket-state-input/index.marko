<label.form-label>
    Status
        <select on-change("valueChanged")>
            <for(ticketState in state.ticketStates)>
                <option key=ticketState.ID value=ticketState.ID selected=(state.stateId === ticketState.ID)>
                    ${ticketState.Name}
                </option>
            </for>
        </select>
</label>
