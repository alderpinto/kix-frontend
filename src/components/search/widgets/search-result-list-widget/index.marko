<if(state.loading)>
    <loading-spinner/>
</if>
<else>
    <widget instanceId=state.instanceId title=state.resultTitle>
        <@icon>
            <icon icon=state.resultIcon/>
        </@icon>
        <@headerContent>
            <filter on-filter('filter')/>
        </@headerContent>
        <@content>
            <if(state.resultTable && !state.noSearch)>
                <standard-table standardTable=state.resultTable/>
            </if>
            <else>Keine Suche verf√ºgbar.</else>
        </@content>
    </widget>
</else>
