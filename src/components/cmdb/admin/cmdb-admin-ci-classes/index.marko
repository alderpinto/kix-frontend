<widget instanceId=state.instanceId title=state.title actions=state.actions icon="kix-icon-gears">
    <@headerFilter>
        <filter filterCount=state.filterCount predefinedFilter=state.predefinedTableFilter
            predefinedFilterPlaceholder="Alle CMDB Klassen" on-filter('filter')/>
    </@headerFilter>
    <@content>
        <if(state.table)>
            <standard-table standardTable=state.table key="cmdb-ci-classes-table"/>
        </if>
    </@content>
</widget>
