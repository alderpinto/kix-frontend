<if(state.show)>
    <div.dialog-main>
        <widget isDialog=true title="Neues Objekt erstellen" minimizable=false>
            <@content>
                <tab-container tabWidgets=state.dialogWidgets minimizable=false/>
                <div.dialog-footer>
                    <version/>
                </div>
            </@content>
        </widget>
        <span.kix-icon-close.close-button on-click('closeDialog')/>
    </div>
</if>
