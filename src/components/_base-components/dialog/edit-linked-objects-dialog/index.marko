<if(!state.loading)>
    <widget instanceId="edit-linked-objects-dialog">
        <@content>
            <div.edit-linked-objects-dialog-container>
                <widget instanceId="link-objects-preview-table" title=component.getResultTitle()>
                    <@content>
                        <if(state.table)>
                            <div.linkObjectsFilter>
                                <filter predefinedFilter=state.predefinedTableFilter predefinedFilterPlaceholder="Alle Objekte" on-filter('filter')/>
                            </div>
                            <standard-table standardTable=state.table/>
                        </if>
                    </@content>
                </widget>
                <div.dialog-button-container>
                    <!-- Löschen- und Verknüpfen-Anlegen-Button -->
                </div>
            </div>
            <div.dialog-button-container>
                <button.cancel-button type="button" on-click("cancel") title="Abbrechen">
                    <span.kix-icon-close/>
                    Abbrechen
                </button>
                <button.submit-button type="button" on-click("submit") title="Übernehmen">
                    <span.kix-icon-check/>
                    Übernehmen
                </button>
            </div>
        </@content>
    </widget>
</if>
<else>
    <loading-spinner/>
</else>
