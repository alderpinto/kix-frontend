<widget instanceId="new-ticket-dialog">
    <@content>
        <if(state.prepared && state.formId)>
            <main-form formId=state.formId/>
        </if>
        <div.dialog-button-container>
            <button.cancel-button
                type="button"
                on-click("cancel")
                title=state.translations["Translatable#Cancel"]
                disabled=state.submitted>
                <span.kix-icon-close/>
                <translation-string pattern="Translatable#Cancel"/>
            </button>
            <button.submit-button
                disabled=(!state.formId || state.submitted)
                type="button"
                on-click("submit")
                title=state.translations["Translatable#Save"]>
                <span.kix-icon-check/>
                <translation-string pattern="Translatable#Save"/>
            </button>
        </div>
    </@content>
</widget>
