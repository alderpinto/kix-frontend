<span.icon>
    <if(state.content)>
        <if(state.base64)>
            <img.svg-icon
                style=input.style
                src=("data:" + state.contentType + ";base64," + state.content)
                title=state.title
                key=state.iconId
                alt=state.title/>
        </if>
        <else>
            <span style=input.style class=state.content title=state.tooltip key=state.iconId>
                <span.path1/>
                <span.path2/>
                <span.path3/>
                <span.path4/>
                <span.path5/>
            </span>
        </else>
    </if>
</span>
