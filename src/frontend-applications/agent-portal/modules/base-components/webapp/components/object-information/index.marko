<if(state.object && state.prepared)>
    <div.object-information class={
        "show-object": state.properties
    }>
        <div.information-list class={
            "flat-list": state.flat
        }>
            <for|property| of=state.properties>
                <div>
                    <object-property-label
                        object=state.object
                        property=property
                        labelProvider=state.labelProvider
                        routingConfiguration=component.getRoutingConfiguration(property)
                        objectId=component.getRoutingObjectId(property)/>
                </div>
            </for>
        </div>
    </div>
</if>
<else>
    <translation-string pattern="Translatable#No data available."/>
</else>
