<if(state.prepared)>
    <div.number-form-input>
        <input.form-control
            type="number"
            value=state.value
            min=state.min
            max=state.max
            step=state.step
            readonly=state.readonly
            disabled=state.readonly
            on-input("valueChanged")
        >
        <span.unit-string><translation-string pattern=state.unitString/></span>
    </div>
</if>
