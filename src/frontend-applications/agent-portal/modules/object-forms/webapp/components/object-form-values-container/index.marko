<if(state.prepared)>
   <div.object-form-values-container>
      <for|formValue| of=state.formValues>
         <if(formValue && !component.isSorted(formValue))>
            <object-form-value formValue=formValue parent=input.parent/>            
         </if>
      </for>
   </div>
</if>
