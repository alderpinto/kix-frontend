<div.search-dashboard>
    <search-criteria-widget key=state.componentKey/>
    <div.search-result-widget>
        <if(state.resultWidget)>
            <table-widget
                key=("search-result-table-" + state.resultWidget[1])
                instanceId=state.resultWidget[0]
                useContext=false
                widgetConfiguration=state.resultWidget[2]
                title=state.resultWidget[3]
                icon=state.resultWidget[4]/>
        </if>
    </div>
</div>
